<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane prefHeight="407.0" prefWidth="600.0" style="-fx-background-color: #002D62;" />
      <BorderPane prefHeight="200.0" prefWidth="200.0" />
      <VBox prefHeight="407.0" prefWidth="139.0" style="-fx-background-color: #A9A9A9;">
         <children>
            <Label alignment="CENTER" prefHeight="46.0" prefWidth="131.0" text="MENU">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font>
            </Label>
            <Button fx:id="scheduleButton" mnemonicParsing="false" onAction="#handleScheduleButton" prefHeight="55.0" prefWidth="118.0" style="-fx-background-color: #002D62;" text="Schedule" textFill="WHITE" translateX="10.0">
               <font>
                  <Font size="16.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="weekButton" mnemonicParsing="false" onAction="#handleWeekButtonAction" prefHeight="55.0" prefWidth="118.0" style="-fx-background-color: #002D62;" text="Week View" textFill="WHITE" translateX="10.0">
               <font>
                  <Font size="16.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="monthButton" mnemonicParsing="false" onAction="#handleMonthButtonAction" prefHeight="55.0" prefWidth="118.0" style="-fx-background-color: #002D62;" text="Month View" textFill="WHITE" translateX="10.0">
               <font>
                  <Font size="16.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="noteButton" mnemonicParsing="false" onAction="#handleNoteButtonAction" prefHeight="55.0" prefWidth="118.0" style="-fx-background-color: #002D62;" text="Notes" textFill="WHITE" translateX="10.0">
               <font>
                  <Font size="16.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="exitButton" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="55.0" prefWidth="118.0" style="-fx-background-color: #002D62;" text="Exit" textFill="WHITE" translateX="10.0">
               <font>
                  <Font size="16.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
