<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="520.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.psu.bw.hallettist261project.FinalNewSchedulerController">
   <children>
      <Pane prefHeight="520.0" prefWidth="800.0" style="-fx-background-color: #002d62; -fx-border-color: #a9a9a9; -fx-border-width: 10;">
         <children>
            <TableView fx:id="eventTable" layoutX="22.0" layoutY="90.0" prefHeight="274.0" prefWidth="756.0">
              <columns>
                <TableColumn fx:id="eventNameColumn" prefWidth="158.0" text="Event Name" />
                <TableColumn fx:id="eventDayColumn" prefWidth="176.0" text="Event Day" />
                  <TableColumn fx:id="eventTimeColumn" prefWidth="199.0" text="Event Time" />
                  <TableColumn fx:id="eventTypeColumn" prefWidth="221.0" text="Event Type" />
              </columns>
            </TableView>
            <Label layoutX="269.0" layoutY="14.0" prefHeight="53.0" prefWidth="263.0" text="Scheduler" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="48.0" />
               </font>
            </Label>
            <Button fx:id="scheduleButton" layoutX="626.0" layoutY="427.0" mnemonicParsing="false" onAction="#handleScheduleButtonOnAction" prefHeight="51.0" prefWidth="123.0" style="-fx-border-color: #ffffff; -fx-background-color: #002d62; -fx-background-radius: 5em; -fx-border-radius: 5em; -fx-border-width: 5;" text="Schedule" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font></Button>
            <Button fx:id="readMeAgainButton" layoutX="34.0" layoutY="427.0" mnemonicParsing="false" onAction="#handleReadMeAgainButtonOnAction" prefHeight="51.0" prefWidth="123.0" style="-fx-background-color: #002d62; -fx-border-color: #ffffff; -fx-background-radius: 5em; -fx-border-radius: 5em; -fx-border-width: 5;" text="Read Me" textFill="WHITE">
               <effect>
                  <DropShadow />
               </effect>
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
            <Button fx:id="homeButton" layoutX="339.0" layoutY="427.0" mnemonicParsing="false" onAction="#handleHomeButtonOnAction" prefHeight="51.0" prefWidth="123.0" style="-fx-background-color: #002d62; -fx-border-color: #ffffff; -fx-background-radius: 5em; -fx-border-radius: 5em; -fx-border-width: 5;" text="Home" textFill="WHITE">
               <effect>
                  <DropShadow />
               </effect>
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
